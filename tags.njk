---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: layouts/base.njk
renderData:
  title: "{{ tag | removeHyphens }} | BLM Resources"
permalink: /{{ tag }}/
---
<h1 class="page-heading">{{ tag | removeHyphens }}</h1>

{% set postslist = collections[ tag ] %}
{% include "postslist.njk" %}
